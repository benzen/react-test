{
  "name": "react-test",
  "version": "1.0.0",
  "description": "A toy issue tracker made with react",
  "main": "app.js",
  "keywords": [
    "toy",
    "issue-tracker",
    "react",
    "virtual-dom"
  ],
  "author": "Benjamin Dreux, Alexandre Rimthong",
  "license": "GPL v3",
  "dependencies": {
    "bootstrap": "^3.3.2",
    "browserify": "^8.1.3",
    "envify": "^3.2.0",
    "react": "^0.12.2",
    "reactify": "^1.0.0",
    "watchify": "^2.3.0"
  },
  "scripts": {
    "start": "watchify -o bundle.js -v -d js/*",
    "build": "NODE_ENV=production browserify . | uglifyjs -cm > bundle.min.js"
  },
  "browserify": {
    "transform": [
      "envify",
      "reactify"
    ]
  }
}
